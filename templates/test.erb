package amazon

import "testing"

func Test<%= operation %>(t *testing.T) {
  client, _ := New("AK", "SK", "JP")
  req := client.<%= operation %>(<%= operation %>Parameters{})
  if req == nil {
    t.Error("Expected not nil but got nil")
  }
  Test{client, req.Client}.Compare(t)
}

func Test<%= operation %>BuildQuery(t *testing.T) {
  client, _ := New("AK", "SK", "JP")
  q := client.<%= operation %>(<%= operation %>Parameters{}).buildQuery()
  Test{0, len(q)}.Compare(t)
}
